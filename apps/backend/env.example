# AI-Enhanced Autonomous Content Pipeline - Environment Variables
# Copy this file to .env and fill in your actual values

# Application
APP_NAME="AI-Enhanced Autonomous Content Pipeline"
APP_VERSION="1.0.0"
DEBUG=false

# Database (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# AI Services
OPENROUTER_API_KEY=your-openrouter-api-key-here
LANGSMITH_API_KEY=your-langsmith-api-key-here
LANGSMITH_PROJECT=phoenix-content-pipeline

# Default AI Models
DEFAULT_CONTENT_MODEL=openai/gpt-4
DEFAULT_RESEARCH_MODEL=openai/gpt-3.5-turbo

# API Configuration
API_TIMEOUT=30
MAX_RETRIES=3

# Content Generation
MAX_CONTENT_LENGTH=2000
DEFAULT_TEMPERATURE=0.7

# Security
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001
